package fermyon:spin@2.0.0;

world http-imports {
  import wasi:http/outgoing-handler@0.2.1;
}

world spin-imports {
  import wasi:http/outgoing-handler@0.2.1;
  import llm;
  import redis;
  import postgres;
  import mqtt;
  import mysql;
  import sqlite;
  import key-value;
  import variables;
}

world spin-redis {
  include spin-imports;
  export fermyon:spin/inbound-redis;
}

world spin-http {
  include spin-imports;
  export wasi:http/incoming-handler@0.2.1;
}

world spin-all {
  include spin-redis;
  include spin-http;
}
